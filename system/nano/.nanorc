set autoindent
set smarthome
set tabsize 4
set tabstospaces
set constantshow
set nowrap
set titlecolor white,blue
set statuscolor white,cyan
set errorcolor white,blue
set selectedcolor black,cyan
set stripecolor ,cyan
set numbercolor cyan
set keycolor cyan
set functioncolor cyan
include "/usr/share/nano/*.nanorc"
syntax "default"
color cyan,black ".*"
color blue "\b(True|False|None)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(if|else|elif|for|while|try|except|with|def|class)\b"
color blue "#.*$"
color cyan "\$[a-zA-Z_][a-zA-Z0-9_]*"
syntax "python" "\.py$"
color cyan,black ".*"
color blue "\b(True|False|None|and|or|not|in|is|as|assert|async|await|break|continue|del|except|finally|from|global|import|lambda|nonlocal|pass|raise|return|try|while|with|yield)\b"
color cyan "\b[0-9]+(\.[0-9]+)?\b"
color blue "\"[^\"]*\"|'[^']*'|f\"[^\"]*\"|r\"[^\"]*\"|b\"[^\"]*\""
color cyan "\b(def|class|if|else|elif|for|while|try|except|finally|with|return|break|continue|pass|lambda|async|await)\b"
color blue "#.*$"
color cyan "\b(__[a-zA-Z0-9_]+__)\b"
color blue "\b(print|input|len|range|type|str|int|float|list|dict|set|tuple|bool|bytes|bytearray|open|file|super|self)\b"
syntax "bash" "\.sh$"
color cyan,black ".*"
color blue "\b(if|then|else|elif|fi|case|esac|while|do|done|for|in|select|until|function)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\$[a-zA-Z_][a-zA-Z0-9_]*|\${[a-zA-Z_][a-zA-Z0-9_]*}"
color blue "#.*$"
color blue "\b(echo|export|set|unset|read|shift|test|eval|exec|source|declare|typeset)\b"
syntax "c" "\.(c|h)$"
color cyan,black ".*"
color blue "\b(int|float|double|char|void|return|if|else|for|while|do|switch|case|break|continue|goto|struct|enum|typedef|union|static|const|volatile|extern|inline)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(struct|enum|typedef|union)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(printf|scanf|malloc|free|sizeof|NULL)\b"
syntax "cpp" "\.(cpp|hpp|cc|cxx)$"
color cyan,black ".*"
color blue "\b(int|float|double|char|void|return|if|else|for|while|do|switch|case|break|continue|goto|class|struct|enum|typedef|namespace|template|public|private|protected|static|const|volatile|extern|inline|virtual|override|final)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(class|struct|enum|typedef|namespace|template)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(cout|cin|endl|string|vector|map|set|unique_ptr|shared_ptr)\b"
syntax "java" "\.java$"
color cyan,black ".*"
color blue "\b(public|private|protected|static|final|abstract|class|interface|extends|implements|void|int|float|double|char|boolean|if|else|for|while|do|switch|case|break|continue|return|try|catch|finally|throw|throws|new|synchronized)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(class|interface|enum)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(System|out|println|print|Scanner|String|Integer|Double|ArrayList|HashMap)\b"
syntax "html" "\.html$"
color cyan,black ".*"
color blue "<[^>]+>"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(class|id|style|href|src|alt|title)\b"
color blue "<!--.*-->"
syntax "javascript" "\.js$"
color cyan,black ".*"
color blue "\b(function|var|let|const|if|else|for|while|do|switch|case|break|continue|return|try|catch|finally|throw|new|async|await)\b"
color cyan "\b[0-9]+(\.[0-9]+)?\b"
color blue "\"[^\"]*\"|'[^']*'|`[^`]*`"
color cyan "\b(class|new|this|super|export|import|default|extends)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(console|log|debug|error|warn|fetch|Promise|async|await)\b"
syntax "typescript" "\.ts$"
color cyan,black ".*"
color blue "\b(function|var|let|const|if|else|for|while|do|switch|case|break|continue|return|try|catch|finally|throw|new|async|await|interface|type|enum|public|private|protected|static|readonly)\b"
color cyan "\b[0-9]+(\.[0-9]+)?\b"
color blue "\"[^\"]*\"|'[^']*'|`[^`]*`"
color cyan "\b(class|interface|type|enum|extends|implements)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(console|log|debug|error|warn|fetch|Promise|async|await)\b"
syntax "css" "\.css$"
color cyan,black ".*"
color blue "\b(body|div|span|p|h1|h2|h3|h4|h5|h6|a|img|ul|ol|li|table|tr|td|th)\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(class|id|style|margin|padding|border|color|background|font|display|flex|grid)\b"
color blue "/\*.*\*/"
color cyan "[0-9]+(px|em|rem|%)"
syntax "json" "\.json$"
color cyan,black ".*"
color blue "\b(true|false|null)\b"
color cyan "\b[0-9]+(\.[0-9]+)?\b"
color blue "\"[^\"]*\""
color cyan "\b([a-zA-Z_][a-zA-Z0-9_]*)\s*:"
color blue "//.*$|/\*.*\*/"
syntax "markdown" "\.md$"
color cyan,black ".*"
color blue "^#+.*$"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(\*|_){1,2}[^\*_\n]+\1"
color blue "<!--.*-->"
syntax "yaml" "\.(yaml|yml)$"
color cyan,black ".*"
color blue "\b(true|false|null)\b"
color cyan "\b[0-9]+(\.[0-9]+)?\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "^[a-zA-Z_][a-zA-Z0-9_]*\s*:"
color blue "#.*$"
syntax "xml" "\.xml$"
color cyan,black ".*"
color blue "<[^>]+>"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(xmlns|version|encoding)\b"
color blue "<!--.*-->"
syntax "sql" "\.sql$"
color cyan,black ".*"
color blue "\b(SELECT|FROM|WHERE|INSERT|UPDATE|DELETE|CREATE|ALTER|DROP|TABLE|INDEX|VIEW|JOIN|INNER|LEFT|RIGHT|OUTER|ON|GROUP|BY|ORDER|ASC|DESC|LIMIT|OFFSET)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(COUNT|SUM|AVG|MIN|MAX)\b"
color blue "--.*$|/\*.*\*/"
syntax "go" "\.go$"
color cyan,black ".*"
color blue "\b(func|var|const|type|struct|interface|if|else|for|range|switch|case|break|continue|return|go|defer|import|package)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(struct|interface|type)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(fmt|Println|Printf|Scanf)\b"
syntax "rust" "\.rs$"
color cyan,black ".*"
color blue "\b(fn|let|mut|const|static|if|else|for|while|loop|match|break|continue|return|struct|enum|impl|trait|use|mod|pub|crate)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(struct|enum|impl|trait)\b"
color blue "//.*$|/\*.*\*/"
color blue "\b(println!|format!|vec!)\b"
syntax "php" "\.php$"
color cyan,black ".*"
color blue "\b(function|class|interface|public|private|protected|static|const|if|else|for|while|do|switch|case|break|continue|return|try|catch|finally|throw|new|echo|print)\b"
color cyan "\b[0-9]+\b"
color blue "\"[^\"]*\"|'[^']*'"
color cyan "\b(class|interface|trait)\b"
color blue "//.*$|/\*.*\*/|#[^\n]*"
color blue "\b(\$[a-zA-Z_][a-zA-Z0-9_]*)\b"
